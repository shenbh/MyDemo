<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="toptabdemo.ab.com.toptabdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".normalindicator.TopTabActivity"
            android:theme="@style/StyledIndicators" />
        <activity
            android:name=".underlinepageindicator.ULActivity"
            android:theme="@style/StyledIndicators"></activity>
        <activity
            android:name=".bottomtab.BottomTabActivity"
            android:label="@string/title_activity_bottom_tab"
            android:theme="@style/StyledIndicators">


        </activity>
        <activity
            android:name=".bottomtab.BottomTabCustomActivity"
            android:label="@string/title_activity_bottom_tab"
            android:theme="@style/StyledindicatorsPic"></activity>
    </application>

</manifest>